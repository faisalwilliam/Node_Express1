      <nav class="px-2 mt-5 d-flex justify-content-between" aria-label="breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/">Home</a></li>
          <li class="breadcrumb-item active" aria-current="page"><%= obj.firstName %> <%= obj.lastName %></li>
        </ol>
        <p>ID: <%= obj._id %></p>
      </nav>

      <div class="card mx-2">
        <div class="card-header">
          User Details
        </div>
        <ul class="list-group list-group-flush">
          <li class="list-group-item">
            <div class="row">
              <div class="col-3 fw-bold">First Name:</div>
              <div class="col"><%= obj.firstName %></div>
            </div>
          </li>
          <li class="list-group-item">
            <div class="row">
              <div class="col-3 fw-bold">Last Name:</div>
              <div class="col"><%= obj.lastName %></div>
            </div>
          </li>
          <li class="list-group-item">
            <div class="row">
              <div class="col-3 fw-bold">Email:</div>
              <div class="col"><%= obj.email %></div>
            </div>
          </li>
          <li class="list-group-item">
            <div class="row">
              <div class="col-3 fw-bold">Phone Number:</div>
              <div class="col"><%= obj.phoneNumber %></div>
            </div>
          </li>
          <li class="list-group-item">
            <div class="row">
              <div class="col-3 fw-bold">Age:</div>
              <div class="col"><%= obj.age %></div>
            </div>
          </li>
          <li class="list-group-item">
            <div class="row">
              <div class="col-3 fw-bold">Country:</div>
              <div class="col"><%= obj.country %></div>
            </div>
          </li>
          <li class="list-group-item">
            <div class="row">
              <div class="col-3 fw-bold">Gender:</div>
              <div class="col"><%= obj.gender %></div>
            </div>
          </li>
          <li class="list-group-item">
            <div class="row">
              <div class="col-3 fw-bold">Created At:</div>
              <div class="col"><%= obj.createdAt ? obj.createdAt.toDateString() : "" %></div>
            </div>
          </li>
        </ul>
      </div>

    </section>
