  <main class="d-flex flex-nowrap">
    <div id="sidebarMenu" class="offcanvas-md offcanvas-start d-flex flex-column flex-shrink-0 p-3 text-bg-dark"
      style="width: 280px;" tabindex="-1" aria-labelledby="sidebarMenuLabel">
      <div class="offcanvas-header d-md-none">
